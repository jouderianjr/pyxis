{
  "name": "pyxis-elm",
  "version": "1.0.0",
  "private": false,
  "license": "ISC",
  "description": "",
  "author": "Alessio Santangelo <alessio.santangelo@prima.it>",
  "homepage": "https://github.com/primait/pyxis-react#readme",
  "main": "lib/pyxis-elm.js",
  "scripts": {
    "build" : "yarn example:build",
    "example:build" : "./node_modules/.bin/elm make src/Examples/Form/Main.elm",
    "example:serve" : "./node_modules/.bin/elm reactor src/Examples/Form/Main.elm",
    "preexample:serve" : "yarn reset-dist",
    "elmbook:build": "elm make src/Stories/Book.elm --output=dist/index.html",
    "elmbook:serve": "elm-book src/Stories/Book.elm --dir=./dist -- --output=dist/index.html --debug",
    "generate:icons": "ts-node --project scripts/tsconfig.json ./scripts/generateIcons.ts && ./node_modules/.bin/elm-format src/Components/IconSet.elm --yes",
    "preelmbook:build": "yarn reset-dist",
    "preelmbook:serve": "yarn reset-dist",
    "reset-dist": "mkdir -p dist && elm make && cp ../pyxis-scss/dist/pyxis.css ./dist/pyxis.css && cp -r ./assets ./dist/assets",
    "review": "./node_modules/.bin/elm-review src",
    "review:fix": "./node_modules/.bin/elm-review src --fix",
    "test": "./node_modules/.bin/elm-test"
  },
  "dependencies": {
    "elm": "^0.19.1-5",
    "elm-book": "^1.0.1",
    "elm-format": "^0.8.5",
    "elm-review": "^2.7.0",
    "elm-test": "^0.19.1-revision7"
  }
}
